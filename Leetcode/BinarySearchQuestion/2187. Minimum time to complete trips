long long minumumtime(vector<int>time,int totaltrips)
{
    long long left=0;
    long long right=(long long)*min_element(time.begin(),time.end())*totaltrips;
    while(left<=right)
    {
        long long mid=left+(right-left)/2;
        long long trips=0;
        for(auto bus:time)
        {
             trips+=mid/bus;
        }
     if(totaltrips<=trips)
        {
          right=mid-1;
        }
        else
        {
            left=mid+1;
        }
        
    }return left;
}
int main()
{
    vector<int>time={1,2,3};
    int totaltrips=5;
   long long ans= minumumtime(time,totaltrips);
   cout<<ans<<" ";
}
